<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kh.team6.dao.MemberDAO">
	<insert id="insertMember">
	    insert into member values
	    (#{member.me_id},#{member.me_birth},#{member.me_email},#{member.me_phone},#{member.me_pw},#{member.me_address},#{member.me_name},"이용중","회원")
	</insert>
	<select id="selectMember" resultType="kr.kh.team6.model.vo.MemberVO">
	    select * from member where me_id=#{id}
	</select>
</mapper>